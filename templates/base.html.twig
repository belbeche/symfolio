<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="theme-color" content="#ffffff">
		<meta name="description" content="creation site pas cher">
		<meta name="keywords" content="DreamSTUDIO , site internet , conception">
		<title>
			{% block title %}Wee portfolio
			{% endblock %}
		</title>
		<link
		rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
		<!-- Axentix CSS minified version -->
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.3.0/dist/css/axentix.min.css">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>
	<header id="header">
		<div class="container">
			{% if app.user == false %}
				<div class="logo">
					<a href="{{ path('index')}}"><img src="images/logo.png" alt="dreamSTUDIO"></a>
				</div>
			{% else %}
				<div class="logo">
					<a href="{{ path('index_logged')}}"><img src="images/logo.png" alt="dreamSTUDIO"></a>
				</div>
			{% endif %}
			<nav id="nav">
				<div class="opener-holder">
					<a href="#" class="nav-opener">
						<span></span>
					</a>
				</div>
				{% if is_granted('ROLE_USER') %}
					<a href="{{ path('app_profil')}}" class="btn btn-info rounded">Profile</a>
				{% elseif is_granted('ROLE_ADMIN') %}
					<a href="#" class="btn btn-danger rounded">Admin</a>
				{% else %}
					<a href="{{ path('app_login')}}" class="btn btn-primary rounded">Espace Membre</a>
				{% endif %}
				{% if app.user == false %}
					<div class="nav-drop">
						<ul>
							<li class="active visible-sm visible-xs">
								<a href="{{path('index')}}">Accueil</a>
							</li>
							<li>
								<a href="#about">À propos</a>
							</li>
							<li>
								<a href="#services">Mes compétences</a>
							</li>
							<li>
								<a href="#projects">Catalogues</a>
							</li>
							<li>
								<a href="#contacts">Contact</a>
							</li>
						</ul>
						<div class="drop-holder visible-sm visible-xs">
							<span>Suivez moi</span>
							<ul class="social-networks">
								<li>
									<a href="https://github.com/belbeche">
										<i class="fab fa-github"></i>
									</a>
								</li>
								<li>
									<a href="https://www.linkedin.com/in/walid-belbeche/">
										<i class="fab fa-linkedin"></i>
									</a>
								</li>
								<li>
									<a href="https://www.facebook.com/waloup.elyana.5/">
										<i class="fab fa-facebook-square"></i>
									</a>
								</li>
							</ul>
						</div>
					{% else %}
						<div class="nav-drop">
							<ul>
								<li class="active visible-sm visible-xs">
									<a href="{{path('index_logged')}}">Accueil</a>
								</li>
								<li>
									<a href="#about">Réalisations</a>
								</li>
								<li>
									<a href="{{ path('blog')}}">Blog</a>
								</li>
								<li>
									<a href="#projects">Catalogues</a>
								</li>
								<li>
									<a href="{{ path('prise_de_rendez_vous') }}">Rendez-vous</a>
								</li>
							</ul>
							<div class="drop-holder visible-sm visible-xs">
								<span>Suivez moi</span>
								<ul class="social-networks">
									<li>
										<a href="https://github.com/belbeche">
											<i class="fab fa-github"></i>
										</a>
									</li>
									<li>
										<a href="https://www.linkedin.com/in/walid-belbeche/">
											<i class="fab fa-linkedin"></i>
										</a>
									</li>
									<li>
										<a href="https://www.facebook.com/waloup.elyana.5/">
											<i class="fab fa-facebook-square"></i>
										</a>
									</li>
								</ul>
							</div>
						{% endif %}
					</div>
				</nav>
			</div>
		</div>
	</html>
</html></html></header><body>{% block body %}
<div class="wrapper"></div>{% endblock %}<!-- FOOTER -->{% block javascripts %}
<script src="http://nversios.com/assets/js/jquery-1.11.2.min.js"></script>
<script src="http://nversios.com/assets/js/bootstrap.js"></script>
<script src="http://nversios.com/assets/js/jquery.main.js"></script>
<script src="https://kit.fontawesome.com/e73a7a7bd1.js" crossorigin="anonymous"></script>
<script src="http://nversios.com/js/foundation.js"></script>
<!-- Axentix JS minified version -->
<script src="https://cdn.jsdelivr.net/npm/axentix@1.3.0/dist/js/axentix.min.js"></script>

{{ encore_entry_script_tags('app') }}{% endblock %}</body></html></html>
